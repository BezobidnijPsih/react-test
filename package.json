{
  "name": "react-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootswatch": "^3.3.7",
    "fetch": "^1.1.0",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^16.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14",
    "styled-components": "^2.2.2",
    "superagent": "^3.6.3"
  },
  "devDependencies": {
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.3",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.7.4",
    "prettier-eslint": "^8.2.1",
    "prettier-eslint-cli": "^4.4.0",
    "raf": "^3.4.0"
  },
  "scripts": {
    "precommit": "npm run lint",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --setupTestFrameworkScriptFile=./src/app/setupTests.js",
    "test-c": "react-scripts test --env=jsdom --setupTestFrameworkScriptFile=./src/app/setupTests.js --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint --cache --ignore-path .gitignore --ext .jsx,.js src"
  },
  "pre-commit": [
    "lint"
  ],
  "jest": {
    "collectCoverageFrom": [
      "**/src/**.**",
      "!**/registerServiceWorker.js",
      "!**/index.js"
    ],
    "coverageReporters": [
      "cobertura",
      "text"
    ]
  }
}
